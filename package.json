    "build": "next build",
    "db:generate": "bun run --env-file .env.local drizzle-kit generate",
    "db:migrate": "bun run --env-file .env.local tsx ./src/server/db/migrate.ts",
    "db:push": "bun run drizzle-kit push:pg",
    "db:studio": "bun run --env-file .env.local drizzle-kit studio",
    "db:seed": "bun --bun run --env-file .env.local --print tsx src/server/db/seed/index.ts",
    "db:start": "bun run sh start-database.sh",
    "db:stop": "bun run sh stop-database.sh",
    "db:reset": "bun run db:stop; bun run db:start; sleep 2; bun run db:migrate; bun run db:seed",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start"
